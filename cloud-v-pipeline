node("sf1-1"){
  stage("Clone repo sf1-1"){
    checkout scm
  }
  stage("Compile helloworld.c sf1-1"){
    sh'''#!/bin/bash
      gcc ./helloworld.c -o helloworld.o
    '''
  }
  stage("Run helloworld.o"){
    sh'''#!/bin/bash
      ./helloworld.o
    '''
  }
  stage("Clean Workspace sf1-1"){
    sh'''
      rm -rf "${WORKSPACE}"/*
    '''
  }
}

